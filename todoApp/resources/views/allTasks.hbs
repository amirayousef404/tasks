{{>header}}
{{>nav}}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Title</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
      <th scope="col">Show</th>
      <th scope="col">Change status</th>
    </tr>
  </thead>
  <tbody>
{{#each data}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.title}}</td>
      <td><a href="/editSingle/:id"><button type="button" class="btn btn-success">Edit</button></a></td>
      <td><a href="/delete/:id"><button type="button" class="btn btn-danger">Delete</button></a></td>
      <td><a href="/showSingle/{{this.id}}"><button type="button" class="btn btn-primary">Show</button></a></td>
      <td><button type="button" class="btn btn-info">Change status</button></td>
    </tr>
{{/each}}    
  </tbody>
</table>
{{>footer}}